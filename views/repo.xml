<odoo>
    <data>

 <record id="view_list_okwrepo" model="ir.ui.view">
        <field name="name">okwrepolist</field>
        <field name="model">odooknowledge.okwrepo</field>
        <field name="arch" type="xml">
            <tree editable="top">
                <field name="name" string="Nombre"/>
                <field name="url" widget="url" placeholder="ej. https://github.com/OCA/web"/>
            </tree>
        </field>
    </record>
    <record id="action_view_okwrepo" model="ir.actions.act_window">
      <field name="name">okwrepo</field>
      <field name="res_model">odooknowledge.okwrepo</field>
      <field name="view_mode">tree,form</field>
    </record>
    <record id="repo_view_search" model="ir.ui.view">
        <field name="name">odooknowledge.okwrepo.search</field>
        <field name="model">odooknowledge.okwrepo</field>
        <field name="arch" type="xml">
            <search string="Repo">
                <field name="name" filter_domain="['|',('name','ilike',self), ('url','ilike',self)]"/>
            </search>
        </field>
    </record>

    <menuitem id="menu_okwrepo" name="okwrepo" action="action_view_okwrepo"/>



    </data>
</odoo>